package data.dto;

import java.sql.Timestamp;

import org.apache.ibatis.type.Alias;

import lombok.Data;

@Alias("ReviewDto")
@Data
public class ReviewDto {
    private int reviewId;
    private int userId;
    private int movieId;
    private int rating;
    private String content;
    private Timestamp createdAt;
    private Timestamp updatedAt;
}